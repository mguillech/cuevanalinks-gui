function selSearch() {
	var el = document.getElementById("searchTypes");
	if (el.className == "hide") {
		el.className = "";
		var buspos = $(".buscador").offset();
		$(el).css("top",buspos.top).css("left",buspos.left);
	} else {
		el.className = "hide";
	}
}
function changeSearch(id,text) {
	$("#search_cat .tbg").text(text);
	$("#cat").attr("value",id);
	selSearch();
	$("#q").focus();
}
